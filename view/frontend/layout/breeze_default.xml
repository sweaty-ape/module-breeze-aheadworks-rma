<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceBlock name="breeze.js">
      <arguments>
        <argument name="bundles" xsi:type="array">
          <item name="customer" xsi:type="array">
            <item name="items" xsi:type="array">
              <item name="awRmaButtonControl" xsi:type="array">
                <item name="path" xsi:type="string">SweatyApe_BreezeAheadworksRma/js/button-control</item>
              </item>
              <item name="awRmaSelectOrderForm" xsi:type="array">
                <item name="path" xsi:type="string">SweatyApe_BreezeAheadworksRma/js/customer/request/new/select-order-form</item>
              </item>
              <item name="awRmaPolicyLink" xsi:type="array">
                <item name="path" xsi:type="string">SweatyApe_BreezeAheadworksRma/js/customer/request/new/policy-link</item>
              </item>
              <item name="awRmaRequestItemManagement" xsi:type="array">
                <item name="path" xsi:type="string">SweatyApe_BreezeAheadworksRma/js/customer/request/new/request-item-management</item>
              </item>
              <item name="awRmaRequestItemMassAction" xsi:type="array">
                <item name="path" xsi:type="string">SweatyApe_BreezeAheadworksRma/js/customer/request/new/request-item-massaction</item>
              </item>
              <item name="awRmaOrderItemsMultipleField" xsi:type="array">
                <item name="path" xsi:type="string">SweatyApe_BreezeAheadworksRma/js/customer/request/new/order-items-multiple-field</item>
              </item>
            </item>
          </item>
        </argument>
      </arguments>
    </referenceBlock>
  </body>
</page>
